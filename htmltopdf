(async () => {
    const browser = await puppeteer.launch();
    const page = await browser.newPage();
    await page.goto(`${__dirname}/uploads/cert.html`, {
      waitUntil: 'networkidle2',
    });
    await page.pdf({ path: `${__dirname}/uploads/cert.pdf`, format: 'a4' });

    await browser.close();
  })();